{
  "prisma.showPrismaDataPlatformNotification": false,
  "prisma.showPrismaDataPlatformOnboarding": false,
  "files.associations": {
    "*.prisma": "prisma"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
